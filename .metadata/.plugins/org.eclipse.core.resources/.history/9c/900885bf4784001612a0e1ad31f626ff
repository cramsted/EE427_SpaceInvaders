#define packword8(b7,b6,b5,b4,b3,b2,b1,b0) \
((b7  << 7 ) | (b6  << 6 ) | (b5  << 5 ) | (b4  << 4 ) | (b3  << 3 ) | (b2  << 2 ) | (b1  << 1 ) | (b0  << 0 ) )

const int s_8x8[] =
{
		packword8(0,0,0,0,0,0,0,0),
		packword8(0,0,0,0,0,0,0,0),
		packword8(0,0,0,0,0,0,0,0),
		packword8(0,0,0,0,0,0,0,0),
		packword8(0,0,0,0,0,0,0,0),
		packword8(0,0,0,0,0,0,0,0),
		packword8(0,0,0,0,0,0,0,0),
		packword8(0,0,0,0,0,0,0,0)
};

